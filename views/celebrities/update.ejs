<h2>Edit Celebrity</h2>

<form action="new" method="POST">
  <label for="name">Name</label>
  <input type="text" id="name" name="<%= celibrity.name %>">

  <label for="occupation">Occupation</label>
  <input type="text" name="occupation" id="<%= celebrity.occupation %>">

  <label for="catch-phrase">Catch Phrase</label>
  <input type="text" name="catchPhrase" id="<%= celibrity.catchPhrase %>">

  <button type="submit">Edit Celebrity</button>
</form>

<a href="/celebrities/:id">Celebrity id</a>
